{
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "extends": ["eslint:recommended"],
    "plugins": ["prettier", "eslint-plugin-prettier"],
    "env": { "node": true, "es6": true },
    "rules": {
        "no-unused-vars": "warn",
        "quotes": ["error", "single", { "avoidEscape": true }],
        "prettier/prettier": "error"
    }
}
